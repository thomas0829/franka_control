# collecting data
python openrt/scripts/collect_demos_real_oculus.py exp_id=green_block_corner episodes=10 split=train

# replay collected data
python openrt/scripts/replay_demos_real.py exp_id=green_block_corner_blocking training.dataset_path=data/green_block_corner_blocking robot.blocking_control=true

# convert data actions -> delta (blocking)
convert_np_to_blocking.ipynb

# train the model
python training/weird_bc/train_script.py exp_id=green_block_corner2_blocking training.dataset_paths=data/green_block_corner2_blocking training.image_keys=["215122255213_rgb"] training.state_keys=["lowdim_ee","lowdim_qpos"]

# evaluate the model
python training/weird_bc/eval_script.py --config-name=bc_policy_real exp_id=green_block_corner2_blocking training.dataset_paths=['data/green_block_corner2_blocking'] training.image_keys=["215122255213_rgb"] training.state_keys=["lowdim_ee","lowdim_qpos"] robot.blocking_control=true robot.max_path_length=70

